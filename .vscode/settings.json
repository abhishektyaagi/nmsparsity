{
  "editor.formatOnSave": true,
  "python.formatting.provider": "black",
  "python.formatting.blackArgs": [
    "--line-length",
    "80"
  ],
  "editor.rulers": [
    80,
  ],
  "autoDocstring.docstringFormat": "google",
  "files.insertFinalNewline": true,
  "python.analysis.typeCheckingMode": "off"
}
